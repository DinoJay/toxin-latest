        ont:compound                   <http://toxin.vub.be/resource/compound/HC%20Yellow%20n°9> ;
        ont:compoundLabel              "HC Yellow n°9" ;
        ont:fasting_triglycerides      "in the females, there was a statistically significant increase in triglycerides at 300 mg/kg bw/day " ;
