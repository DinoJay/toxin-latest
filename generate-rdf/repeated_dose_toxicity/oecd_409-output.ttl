<http://toxin.vub.be/resource/test/repeated-dose-toxicity/4>
        a                   ont:Test ;
        ont:compound        <http://toxin.vub.be/resource/compound/triclosan> ;
        ont:gross_necropsy_external_surface_body
                "macroscopic evaluations found no gross pathology for any organ" ;
        ont:guideline       <http://toxin.vub.be/resource/guideline/OECD%20409> ;
        ont:guidelineLabel  "OECD 409" ;
        ont:haematocrit     "haematology was limited to high-dose group animals." ;
        ont:histopathology_all_gross_lesions
                "histopathologic evaluation of all major organs from all animals showed no difference among treated and control animals." ;
        ont:observation_organ_weight_organ_body_weight
                "organ weight measurements showed no changes in relative or absolute weights." ;
        ont:observation_other_signs_of_toxicity
                "occasional pasty to thin faeces were noted in all groups." ;
        ont:other_enzymes_and_biomarker
                "clinical chemistry evaluations was limited to high-dose group animals." ;
        ont:type            "in vivo" .

