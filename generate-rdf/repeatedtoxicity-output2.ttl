        ont:dermal                 false ;
        ont:description_of_pod     "noael mg/kg bw/d" ;
        ont:dose_levels            "0, 5, 20, 50" ;
        ont:dose_levels_unity      "yes, gavage" , "mg/kg bw/d" ;
